	F.Intrinsic.Control.If(V.DataTable.dtNotes.Exists,=,True)
		F.Data.DataTable.Close("dtNotes")
	F.Intrinsic.Control.EndIf
	'Create DataTable
	V.Local.sSQL.Set("SELECT COMPID, OID, CASE WHEN COMPTYPE = 15 THEN VCM.NAME_CUSTOMER WHEN COMPTYPE = 18 THEN PM.NAME_PROSPECT WHEN COMPTYPE = 19 THEN PM.NAME_PROSPECT WHEN COMPTYPE = 10 THEN VVM.NAME_VENDOR END,CASE WHEN COMPTYPE = 15 THEN 'C' WHEN COMPTYPE = 18 THEN 'P' WHEN COMPTYPE = 19 THEN 'S' WHEN COMPTYPE = 10 THEN 'V' END,CASE WHEN NTYPE = 0 THEN 'Main' WHEN NTYPE = 1 THEN 'Contact' WHEN NTYPE = 2 THEN 'Opportunity' END,CT.NAME,CNC.NOTES,CNC.LAST_CHG_BY,CNC.LAST_CHG_DATETIME,COMPTYPE FROM CRM_NOTES_COMBINED CNC LEFT JOIN CONTACT CT ON CT.ALT_ID = CNC.CID LEFT JOIN V_CUSTOMER_MASTER VCM ON CNC.COMPID = VCM.CUSTOMER LEFT JOIN PROSPECT_MASTER PM ON CNC.COMPID = PM.PROSPECT LEFT JOIN V_VENDOR_MASTER VVM ON CNC.COMPID = VVM.VENDOR WHERE CNC.COMPID <> ''  AND CNC.NTYPE = 2 ORDER BY COMPID;")
	F.Data.DataTable.CreateFromSQL("dtNotes","con",V.Local.sSQL,True)
	'Adding columns for Opportunity Description, Close Reason, and Close Date as requested by customer. FEQ 6/16/17
	'The following data table will be used to create dictionaries to add the opportunity description, opportunity close date, and opportunity close reason.
	F.Data.DataTable.CreateFromSQL("dtForDics", "con", "SELECT OID, ODESC, OCLOSEDATE, OCLOSEREASON from CRM_OPP_MASTER")
	
	'For the Opportunity Description
	F.Data.DataTable.AddColumn("dtNotes", "Opportunity Description", "String")
	F.Data.Dictionary.CreatefromDataTable("OppDescDic", "dtForDics", "OID", "ODESC")
	F.Data.Dictionary.SetDefaultReturn("OppDescDic", "")
	F.Data.DataTable.FillFromDictionary("dtNotes", "OppDescDic", "OID", "Opportunity Description")
	F.Data.Dictionary.Close("OppDescDic")
		
	'For the Opportunity Close Reason
	F.Data.DataTable.AddColumn("dtNotes", "CloseID", "Long", 0)
	F.Data.Dictionary.CreatefromDataTable("OppCloseReason", "dtForDics", "OID", "OCLOSEREASON")
	F.Data.Dictionary.SetDefaultReturn("OppCloseReason", "0")
	F.Data.DataTable.FillFromDictionary("dtNotes", "OppCloseReason", "OID", "CloseID")
	F.Data.Dictionary.Close("OppCloseReason")
		
	F.Data.DataTable.AddExpressionColumn("dtNotes", "CloseIDReal", "String", "IIF(CloseID IS NULL, '0', CloseID)")
		
	F.Data.DataTable.AddColumn("dtNotes", "Close Reason", "String")
	F.Data.Dictionary.CreatefromSql("OppReason", "con", "Select SEQ, VALUE from CRM_OPP_CLSRSN", 1)
	F.Data.Dictionary.SetDefaultReturn("OppReason", "")
	F.Data.DataTable.FillFromDictionary("dtNotes", "OppReason", "CloseIDReal", "Close Reason")
	F.Data.Dictionary.Close("OppReason")
	
	'For the Opportunity Closind Date
	F.Data.DataTable.AddColumn("dtNotes", "Closed Date", "String")
	F.Data.Dictionary.CreatefromDataTable("OppCloseDate", "dtForDics", "OID", "OCLOSEDATE")
	F.Data.Dictionary.SetDefaultReturn("OppCloseDate", "")
	F.Data.DataTable.FillFromDictionary("dtNotes", "OppCloseDate", "OID", "Closed Date")
	F.Data.Dictionary.Close("OppCloseDate")
			
	'Create GridView
	Gui.frmCRM.GsGCNotes.AddGridViewFromDataTable("gvNotes","dtNotes")
